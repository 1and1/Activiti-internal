<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn"
             targetNamespace="Examples">

    <process id="intermediateTimerEventExampleB" name="Timer intermediate event exampleB">

        <startEvent id="theStartTimerB"/>
        <sequenceFlow id="flow1" sourceRef="theStartTimerB" targetRef="timerB"/>

        <intermediateCatchEvent id="timerB">
            <timerEventDefinition>
                <timeDuration>PT0.000000000000001S</timeDuration>
            </timerEventDefinition>
        </intermediateCatchEvent>

        <sequenceFlow id="flow2" sourceRef="timerB" targetRef="theEnd"/>

        <endEvent id="theEnd"/>

    </process>    

</definitions>
